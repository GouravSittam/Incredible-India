  <style>
    /* grv */
    .grv {
      padding: 6vh;
      display: flex;
      justify-content: center;
      align-items: center;
      text-align: center;
    }

    /* Hero Section */

    .hero {
      position: relative;
      height: 600px;
      overflow: hidden;
    }

    .hero video {
      position: absolute;
      top: 50%;
      left: 50%;
      min-width: 100%;
      min-height: 100%;
      width: auto;
      height: auto;
      z-index: -100;
      transform: translateX(-50%) translateY(-50%);
      background-size: cover;
      transition: 1s opacity;
    }

    .hero .hero-content {
      position: relative;
      z-index: 1;
      display: flex;
      flex-direction: column;
      align-items: center;
      justify-content: center;
      height: 100%;
      color: white !important;
      text-shadow: 1px 1px 2px rgba(0, 0, 0, 0.5);
    }

    .hero h1 {
      font-size: 4rem;
      font-weight: bold;
      margin-bottom: 1rem;
    }

    .hero p {
      font-size: 1.5rem;
      margin-bottom: 2rem;
    }

    /* Featured Destinations */
    .featured-destinations {
      padding: 50px 0;
    }

    .destination-card {
      margin-bottom: 30px;
      position: relative;
      overflow: hidden;
    }

    .destination-card img {
      height: 300px;
      object-fit: cover;
      transition: transform 0.3s ease-in-out;
    }

    .destination-card:hover img {
      transform: scale(1.1);
    }

    .destination-card .overlay {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background-color: rgba(0, 0, 0, 0.5) !important;
      display: flex;
      flex-direction: column;
      justify-content: center;
      align-items: center;
      color: white !important;
      opacity: 0;
      transition: opacity 0.3s ease-in-out;
    }

    .destination-card:hover .overlay {
      opacity: 1;
    }

    .overlay h4 {
      font-size: 1.8rem;
      font-weight: bold;
      margin-bottom: 0.5rem;
    }

    .overlay p {
      font-size: 1.2rem;
      margin-bottom: 1rem;
    }

    .overlay a {
      color: white !important;
      font-size: 1.2rem;
      text-decoration: none;
    }

    .overlay a:hover {
      color: #007bff !important;
    }

    /* About Section */
    .about {
      padding: 50px 0;
      background-color: #f8f9fa !important;
    }

    .about h2 {
      font-size: 2.5rem;
      font-weight: bold;
      margin-bottom: 1.5rem;
    }

    .about p {
      font-size: 1.2rem;
      line-height: 1.8;
    }

    /* Testimonials Section */
    .testimonials {
      padding: 50px 0;
    }

    .testimonial-card {
      background-color: #f8f9fa !important;
      padding: 2rem;
      border-radius: 0.5rem;
      text-align: center;
    }

    .testimonial-card img {
      width: 80px;
      height: 80px;
      border-radius: 50%;
      margin-bottom: 1rem;
    }

    .testimonial-card p {
      font-size: 1.2rem;
      font-style: italic;
      margin-bottom: 1rem;
    }

    .testimonial-card h4 {
      font-size: 1.5rem;
      font-weight: bold;
    }

    .testimonial-card h5 {
      font-size: 1.2rem;
      color: #6c757d;
    }

    /* navbar */
    .navbar {
      height: 5rem;
      background-color: white !important;
    }

    .fa-centos {
      color: #735DA5;
      font-size: 3rem;
    }

    .nav-link {
      color: #222222 !important;
    }

    /* hero  */
    .container-fluid {
      padding: 0%;
    }

    /* packages */
    .package-card {
      border: 1px solid #ddd !important;
      border-radius: 5px;
      overflow: hidden;
      transition: transform 0.3s ease-in-out;
    }

    .package-card:hover {
      transform: translateY(-10px);
      box-shadow: 0 10px 20px rgba(0, 0, 0, 0.1) !important;
    }

    .package-card img {
      width: 100%;
      height: 200px;
      object-fit: cover;
    }

    .package-card .card-body {
      padding: 20px;
    }

    .package-card h4 {
      font-size: 1.2rem;
      margin-bottom: 10px;
    }

    .package-card p {
      font-size: 0.9rem;
      color: #666 !important;
      margin-bottom: 20px;
    }

    .package-card .btn-primary {
      background-color: #007bff !important;
      border-color: #007bff !important;
    }

    .package-card .btn-primary:hover {
      background-color: #0056b3 !important;
      border-color: #0056b3 !important;
    }

    @media (max-width: 767px) {
